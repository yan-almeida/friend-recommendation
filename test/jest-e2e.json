{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/in-memory-repository/(.*)": "<rootDir>/../libs/in-memory-repository/src/$1",
    "@app/in-memory-repository": "<rootDir>/../libs/in-memory-repository/src"
  }
}